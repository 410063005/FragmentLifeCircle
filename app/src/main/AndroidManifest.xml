<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cm.fragmentlifecircle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SingleChildActivity" />
        <activity android:name=".MultiChildActivity" />
        <activity android:name=".NestChildActivity" />
        <activity android:name=".MultiChildBottomBarActivity" />
        <activity android:name=".StatefulFragmentActivity" />
        <activity android:name=".PausedActivity" />
        <activity android:name=".Paused2Activity" />
        <activity android:name=".CommitTransactionActivity" />
    </application>

</manifest>
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cm.demo_arch">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".room.RoomAddUserActivity" />
        <activity android:name=".room.RoomActivity" />
        <activity android:name=".room.RoomListAllActivity" />
        <activity android:name=".codelabs.WordActivity" />
        <activity android:name=".codelabs.NewWordActivity" />
        <activity android:name=".lifecycle.LifecycleOwnerActivity" />
        <activity android:name=".livedata.LiveDataActivity" />
    </application>

</manifest>